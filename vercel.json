{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/fact-check",
      "dest": "wsgi.py"
    },
    {
      "src": "/fact-check-image",
      "dest": "wsgi.py"
    },
    {
      "src": "/health",
      "dest": "wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}